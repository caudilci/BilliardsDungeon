--player
function make_player()
	player = {}
	player.health = 3
	player.x = 24
	player.y = 60
	--maxpow?
	player.pow = 6
	player.velx = 0
	player.vely = 0
	player.direction = 0
	player.directionx = 1
	player.directiony = 0
	player.sprite = 240
	player.state = 0
end

function draw_player()
	
	if player.state==0 then
		line(player.x+4,player.y+4,
		player.directionx*player.pow+player.x+4,
		player.directiony*player.pow+player.y+4,
		8)
	end
	
	spr(player.sprite, player.x, player.y)
end

-- todo: add acceleration to changing shot direction
function move_player()
	if(player.state == 0) then
		if(btnp(‚¨ÖÔ∏è)) then
		 player.direction += 1/72
		 player.directionx, player.directiony = direction_to_vector(player.direction)
		elseif(btnp(‚û°Ô∏è)) then
			player.direction -= 1/72
			player.directionx, player.directiony = direction_to_vector(player.direction)
		elseif(btnp(‚¨ÜÔ∏è)) then
			player.pow += 2
		elseif(btnp(‚¨áÔ∏è)) then
			player.pow -= 2
		elseif(btnp(üÖæÔ∏è)) then
			player.state=1
			player.velx = player.directionx * player.pow
			player.vely = player.directiony * player.pow
		end
	elseif(player.state == 1) then
	 	if(player.velx == 0 and player.vely == 0) then
	 		player.state=0
	 	else
	 		local newx = player.x + player.velx
	 		local newy = player.y + player.vely
	 		for i=player.x,newx,1 do
	 			for j=player.y,newy,1 do
	 				local hit, map_tile, corner = hit_wall(i,j,8,8)
	 				if hit then
	 					if map_tile == 2 then
	 						if mget(pixeltomapgrid(player.x+9,player.y))==2
	 						or mget(pixeltomapgrid(player.x-1,player.y))==2 then
	 							player.velx = -player.velx
	 						end
	 						if mget(pixeltomapgrid(player.x,player.y+9))==2
	 						or mget(pixeltomapgrid(player.x,player.y-1))==2 then
	 							player.vely = -player.vely
	 						end
	 					else
		 					player.velx = 0
		 					player.vely = 0
		 				end
	 				end
	 				 player.x = i
	 				 player.y = j
	 			end
	 		end
	 		
	 		
	 		player.velx *= 0.96
	 		player.vely *= 0.96
	 		if player.velx <= 0.05 and player.velx >= -0.05
	 		and player.vely <= 0.05 and player.vely >= -0.05
	 		then
	 			player.velx, player.vely = 0,0
	 		end
	 	end
	end
end

--move player inbounds if out
function check_oob()
	if player.x > 112 then
		player.x=112
	elseif player.x<8 then
		player.x=8
	end
	if player.y > 112 then
		player.y=112
	elseif player.x<8 then
		player.y=8
	end
end

function hit_wall(x,y,w,h)
	local corner = 0
	for i=x,x+w,w do
		corner++
		local map_tile = mget(pixeltomapgrid(i,y))
		local flag = fget(map_tile)
		if flag == 2 then
			return true, map_tile, corner
		end
	end
	
	for i=y,y+h,h do
		corner++
		local map_tile = mget(pixeltomapgrid(x,i))
		local flag = fget(map_tile)
		if flag == 2 then
			return true, map_tile, corner
		end
	end
end
