--main

--setting: cave
--goal: use all items
--genre: rpg
--rules: bounce off walls
--wildcard: fairlytale

--ideas: 
-- top down dungeon crawler
-- player shoots moves like pool ball
-- find items that change how the player moves
-- kill enemies by hitting them
-- hitting enemies bounce them?
-- take damage by falling in pits
-- take damage from enemy attacks while moving slowly
-- pits will respawn player at door

--requirements:
-- pool mechanics/physics
-- combat vs exploring vs dead state machine
-- dungeon generator
-- items with abilities
-- different enemies with abilities
-- turns?

function _init()
 make_player()
 -- normals for corner tiles
 --tiles = [{x=-0.0883686861,y=-0.0883686861},{x=0.0883686861,y=-0.0883686861},{x=-0.0883686861,y=0.0883686861},{x=0.0883686861,y=0.0883686861}]
end

function _draw()
	cls()
	map(0)
	draw_player()
	print(mget(flr((player.x+8)/8),flr((player.y+8)/8)), 2,2,7)
end

function _update()
	move_player()
end



function collide()

	if mget(pixeltomapgrid(player.x+4,player.y+4)) == 3 then
		local vx, vy =
		reflectvec(player.velx, player.vely,
		-0.70710678118, -0.70710678118)
		player.velx = vx
		player.vely = vy
		local gridx,gridy = pixeltomapgrid(player.x, player.y)
		if player.x>gridx*8+2 then
			player.x = gridx*8+2
		end
		if player.y>gridy*8 then
			player.y = gridy*8
		end
	end
	if mget(pixeltomapgrid(player.x+4,player.y+4)) == 4 then
		local vx, vy =
		reflectvec(player.velx, player.vely,
		0.70710678118, -0.70710678118)
		player.velx = vx
		player.vely = vy
	end
	
	--wall bounce
	if player.x > 112 or player.x<8 then
		player.velx = -player.velx
	end
	if player.y > 112 or player.y<8 then
		player.vely = -player.vely
	end
	
	
	--move player inbounds if out
	if player.x > 112 then
		player.x=112
	elseif player.x<8 then
		player.x=8
	end
	if player.y > 112 then
		player.y=112
	elseif player.x<8 then
		player.y=8
	end
end
