--helper functions

function normalize_vector(x,y)
	c = sqrt(x^2 + y^2)
	return {x/c,y/c}
end

function direction_to_vector(rad)
	local y = sin(rad)
	local x = cos(rad)
	return x,y
end

function degrees_to_radians(deg)
	return deg*(3.14159/180)
end

function dotpart(vx,vy,nx,ny)
    local dot = vx*nx+vy*ny
    vx = vx - dot*nx
    vy = vy - dot*ny
    return vx,vy
end

function reflectvec(vx, vy, nx, ny)
	local dot = vx*nx+vy*ny
	local x = vx - (2*dot*nx)
	local y = vy - (2*dot*ny)
	return x,y
end

function pixeltomapgrid(x,y)
	return flr(x/8),flr(y/8)
end
